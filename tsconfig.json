{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "build/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "middleware.ts",
    "src/app/types/[marked]/default.jsx",
    "build/dev/types/**/*.ts"
, "src/components/ButtonSubmit/worker.js"  ],
  "exclude": [
    "node_modules"
  ]
}
